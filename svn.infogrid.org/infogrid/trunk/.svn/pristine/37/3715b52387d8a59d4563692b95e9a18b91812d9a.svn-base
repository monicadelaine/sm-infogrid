    <option>text/plain</option>
    <option selected="selected">text/html</option>
    <option>image/gif</option>
   </select>
   <a href="javascript:org_infogrid_jee_taglib_mesh_PropertyValueTag_doRemove( 'VAR1e.propertyvalue.1' )">Remove</a>
  </div>
  <textarea id="VAR1e.propertyvalue.1" name="VAR1e.propertyvalue.1" class="org-infogrid-jee-taglib-mesh-PropertyValueTag-show">Test &lt;b&gt;Text&lt;/b&gt;.</textarea>
  <input id="VAR1e.propertyvalue.1.upload" name="VAR1e.propertyvalue.1.upload" type="file" accept="text/plain,text/html,image/gif" class="org-infogrid-jee-taglib-mesh-PropertyValueTag-hide"/>
 </div>
 <div id="VAR1e.propertyvalue.1-no-value" class="org-infogrid-jee-taglib-mesh-PropertyValueTag-hide">
  Null
  <a href="javascript:org_infogrid_jee_taglib_mesh_PropertyValueTag_doCreate( 'VAR1e.propertyvalue.1', 'Test <b>Text</b>.' )">Create</a>
 </div>
</div>

