<?xml version="1.0" encoding="UTF-8"?>

<standardmodule>
 <name>org.infogrid.authp.test</name>
 <username>Authentication provider (testapp)</username>
 <userdescription>Testing the authentication provider</userdescription>
 <runclass>org.infogrid.authp.test.AllTests</runclass>
 <dependencies>
  <requires name="org.infogrid.util.logging.log4j">
   <parameter name="org.infogrid.util.ResourceHelper.ApplicationResourceBundle" value="org/infogrid/authp/test/ResourceHelper" />
   <parameter name="org.infogrid.util.logging.log4j.ConfigPropertiesFile" value="org/infogrid/authp/test/Log.properties"/>
  </requires>
  <requires name="org.infogrid.testharness.tomcat"/>
  <requires name="org.infogrid.lid.openid"/>
  <requires name="org.infogrid.crypto"/>
 </dependencies>
 <provides>
  <jar>org.infogrid.authp.test.jar</jar>
 </provides>
</standardmodule>
